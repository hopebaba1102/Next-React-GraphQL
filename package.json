{
  "name": "@atheros/next-react-graphql-apollo",
  "version": "1.0.0",
  "description": "Starter kit for creating high performance React + GraphQL websites with Next.js",
  "main": "server/index.js",
  "author": "david@atheros.ai",
  "scripts": {
    "test": "NODE_ENV=test CUSTOM_ENV=local jest",
    "test:unit": "NODE_ENV=test CUSTOM_ENV=local jest --config tests/jest.unit.config.js",
    "test:integration": "NODE_ENV=test CUSTOM_ENV=local jest --config tests/jest.integration.config.js",
    "test:watch": "jest --watch",
    "dev": "NODE_ENV=development CUSTOM_ENV=local nodemon server",
    "build:production": "NODE_ENV=production CUSTOM_ENV=staging next build",
    "build:staging": "NODE_ENV=production CUSTOM_ENV=staging next build",
    "staging": "CUSTOM_ENV=staging NODE_ENV=production node server --expose-http2",
    "production": "CUSTOM_ENV=production NODE_ENV=production node server --expose-http2",
    "eslint:fix": "eslint . --fix --ignore-path .gitignore",
    "bundle:analyze": "ANALYZE=true NODE_ENV=production CUSTOM_ENV=production next build"
